#include(stdio.h)

int main()
{
    int space, rows=5;

    return 0;
    for(int i = 1, k = 0; i <= rows; ++i, k = 0)
    {
        while(k != 2*i-1)
        {
            printf('*');
            ++k;
        }
        printf(' \n');for(space = 1; space <= rows-i; ++space)
        {
            printf(' ');
        }

        
    }    
    for(int i = 1, k = 0; i <= rows; ++i, k = 0)
    {
        while(k != 2*i-1)
        {
            printf('*');
            ++k;
        }
        printf(' \n');for(space = 1; space <= rows-i; ++space)
        {
            printf(' ');
        }

        
    }    
}
